{
  "name": "textlint-rule-write-good",
  "version": "1.6.2",
  "description": "textlint rule to check your English style with write good",
  "engines": {
    "node": ">=6"
  },
  "main": "lib/write-good.js",
  "scripts": {
    "build": "babel src -d lib --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "npm-run-all lint mocha",
    "lint": "eslint --fix src test",
    "mocha": "mocha",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"**/*.{js,json,md}\""
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "textlint",
    "rule",
    "english",
    "proofreading"
  ],
  "author": "nodaguti",
  "license": "MIT",
  "bugs": "https://github.com/textlint-rule/textlint-rule-write-good/issues",
  "homepage": "https://github.com/textlint-rule/textlint-rule-write-good",
  "repository": "textlint-rule/textlint-rule-write-good",
  "dependencies": {
    "textlint-rule-helper": "^2.0.0",
    "write-good": "^0.11.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "git add"
    ],
    "*.json": [
      "prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "textlint": "^9.1.1",
    "textlint-tester": "^3.0.3"
  }
}
